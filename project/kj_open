
  cat > /home/websoft/java_jar/yzgongche_run.sh <<EOF
#!/bin/sh

export JAVA_HOME=/usr/local/java/jdk1.8.0_131
export PATH=$JAVA_HOME/bin:$PATH
java -jar /home/websoft/java_jar/yzgongche/1102/beansprouts.jar > /opt/data/logs/run/yzgongche.log &
EOF

  cat > /usr/lib/systemd/system/yzgongche.service <<EOF
[Unit]
Description=yzgongche.service for yzgongche
After=syslog.target network.target remote-fs.target nss-lookup.target
 
[Service]
Type=forking
ExecStart=/home/websoft/java_jar/yzgongche_run.sh
PrivateTmp=true
 
[Install]
WantedBy=multi-user.target
EOF

chmod 754 /usr/lib/systemd/system/yzgongche.service
chmod 754 /home/websoft/java_jar/yzgongche_run.sh

systemctl daemon-reload
systemctl enable yzgongche.service
systemctl start yzgongche.service
